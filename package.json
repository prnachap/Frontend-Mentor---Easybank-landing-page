{
  "name": "easybank-landing-page-master",
  "version": "1.0.0",
  "description": "![Design preview for the Easybank landing page coding challenge](./design/desktop-preview.jpg)",
  "main": "index.js",
  "dependencies": {
    "node-sass": "^5.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "watch:sass": "node-sass -w sass/ -o dist/css/ --recursive",
    "compile:sass":"node-sass sass/main.scss dist/css/style.comp.css",
    "prefix":"postcss --use autoprefixer -b 'last 10 versions' dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress":"node-sass dist/css/style.prefix.css dist/css/style.min.css --output-style compressed"
  },
